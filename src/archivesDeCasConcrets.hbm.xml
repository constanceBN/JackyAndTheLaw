<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.Jacky.model.ArchivesDeCasConcrets" table="archivesDeCasConcrets">
		<id name="idArchives" type="int" column="IdArchives">
			<generator class="increment" />
		</id>
		<property name="sujetArchives">
			<column name="SujetArchives" />
		</property>
		<property name="contenuArchives">
			<column name="ContenuArchives" />
		</property>
	</class>
</hibernate-mapping>    