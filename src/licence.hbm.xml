<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.Jacky.model.Licence" table="licence">
		<id name="idLicence" type="int" column="IdLicence">
			<generator class="increment" />
		</id>
		<property name="typeLicence">
			<column name="TypeLicence" />
		</property>
		<property name="nomLicence">
			<column name="NomLicence" />
		</property>
		<property name="contenuLicence">
			<column name="ContenuLicence" />
		</property>
	</class>
</hibernate-mapping>    